{
  "compilerOptions": {
    "incremental": true,
    "module": "NodeNext",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "noImplicitOverride": true,
    "outDir": "./dist",
    "allowJs": true
  }
}